
# Morvo AI Marketing Platform - Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# WARNING: Never commit your actual .env.local file to version control

# Core API Configuration
VITE_OPENAI_API_KEY="your-openai-api-key-here"
VITE_API_KEY="your-api-key-here"
VITE_API_KEY_HEADER="X-API-Key"

# Supabase Backend Configuration
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your-supabase-anon-key"
VITE_SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"
VITE_SUPABASE_PROJECT_REF="your-project-ref"
VITE_SUPABASE_ACCESS_TOKEN="your-access-token"

# Morvo API Configuration (use localhost for development)
VITE_MORVO_API_URL="http://localhost:8090"
VITE_MORVO_WS_URL="ws://localhost:8090/ws"

# MCP Configuration for Agent Memory
VITE_MCP_ENABLED="true"
VITE_MCP_MEMORY_TABLE="agent_memories"
VITE_MCP_CONTEXT_TABLE="cross_agent_context"
VITE_MCP_MAX_MEMORIES_PER_AGENT="50"

# Agent Configuration
VITE_M1_AGENT_ID="m1"
VITE_M2_AGENT_ID="m2"
VITE_M3_AGENT_ID="m3"
VITE_M4_AGENT_ID="m4"
VITE_M5_AGENT_ID="m5"

# Language Support
VITE_DEFAULT_LANGUAGE="ar"
VITE_SUPPORTED_LANGUAGES="ar,en"

# Development Configuration
VITE_PORT="8080"
VITE_LOG_LEVEL="INFO"

# Instructions:
# 1. Copy this file to .env.local
# 2. Fill in your actual API keys and configuration
# 3. Make sure .env.local is in your .gitignore
# 4. For production, set these variables in your hosting platform
